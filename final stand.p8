	--enemy code
	
	function make_zombie()
		z={}
		
		z.hp=50
		z.attack=5  
		
		z.x=50*8
		z.y=50*10
		
		z.dx=0 
		z.dy=0 
		
		z.w=7
		z.h=7
		
		z.flip=false
	
		z.anim=0
		z.sprite=48 
	end
	
	function move_zombie()
	
		dx = z.x-p.x
		if (dx<0) dx=dx*-1
		dy = z.y-p.y
		if (dy<0) dy=dy*-1
		if dx>=dy then
		 if (p.x<z.x) z.left=true z.flip=false z.dx-=0.5
	  if (p.x>z.x) z.left=true z.flip=true z.dx+=0.5
	 else
	  if (p.y<z.y) z.dy-=0.5
	  if (p.y>z.y) z.dy+=0.5
	 end
	
	
		 z.x+=z.dx
	
	 
			z.y+=z.dy
		
	
		z.dx,z.dy=0,0
	end
	
	function zombie_animate()
		if z.left then
		 if time()-z.anim>.48 then
	  	z.anim=time()
	  	z.sprite+=1
	  	if z.sprite>53 then
	   	z.sprite=50
	   end
	  end
	 else
		 if time()-z.anim>.51 then
	  	z.anim=time()
	  	z.sprite+=1
	  	if z.sprite>49 then
	   	z.sprite=48 
	   end
	  end
		end
	end
