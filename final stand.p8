--player functions

function make_player()
	p={}  
	
	p.hp=100
	
	p.x=24
	p.y=60
	
	p.dx=0 
	p.dy=0 
	
	p.w=7
	p.h=7
	
	p.flip=false
	 
	p.anim=0
	p.sprite=1 
end

function move_player()


	if (btn(⬅️)) then
	 p.dx-=1
	 p.left=true
	 p.flip=false
 end
	if (btn(➡️)) then 
	p.dx+=1
	p.left=true
	p.flip=true
	end
	if (btn(⬆️)) then p.dy-=1 end
	if (btn(⬇️)) then p.dy+=1 end
	
	if p.left
	and not btn(⬅️)
	and not btn(➡️)
	then p.left=false
end

 if (can_move(p.x+p.dx,p.y,p.w,p.h)) then
	 p.x+=p.dx
 end
 
 if (can_move(p.x,p.y+p.dy,p.w,p.h)) then
		p.y+=p.dy
	end
	
	p.dx,p.dy=0,0
end



function player_animate()
	if p.left then
	 if time()-p.anim>.1 then
  	p.anim=time()
  	p.sprite+=1
  	if p.sprite>6 then
   	p.sprite=3
   end
  end
 else
	 if time()-p.anim>.2 then
  	p.anim=time()
  	p.sprite+=1
  	if p.sprite>2 then
   	p.sprite=1 
   end
  end
	end
end
		
		
