function _init()
	make_player()
	make_zombie()
	game_win=false
	game_over=false
	win=7
	stopwatch = 0
end

function _update60()
	if (not game_over) then
		move_player()
		move_zombie()
		player_animate()
		zombie_animate()
		check_win_lose()
		stopwatch += 1
	else
		if (btn(❎)) extcmd("reset")
	end
end

function _draw()
	cls()
	if (not game_over) then
		draw_map()
		draw_player()
		draw_zombie()
	else
		draw_win_lose()
	end
end
